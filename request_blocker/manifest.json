{
  "manifest_version": 3,
  "name": "Basic Request Blocker",
  "description": "Base Level Extension",
  "version": "1.0",
  "action": {
    "default_icon": "icon.png"
  },
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "blocker",
        "enabled": true,
        "path": "blocker_rule.json"
      }
    ]
  },
  "permissions": ["declarativeNetRequest"]
}
