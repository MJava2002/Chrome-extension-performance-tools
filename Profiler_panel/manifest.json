{
  "name": "ProfilerPanel",
  "description": "Add bar",
  "version": "1.0",
  "manifest_version": 3,
  "devtools_page": "devtools.html",
  "permissions": [
      "debugger",
      "tabs",
      "activeTab"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {},
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "web_accessible_resources": [
    {
      "resources": ["lib/d3.v7.min.js", "lib/d3-flamegraph.min.js", "styles/d3-flamegraph.css"],
      "matches": ["<all_urls>"]
    }
  ]
}